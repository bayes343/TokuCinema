{
  "name": "toku-cinema",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --prod",
    "test": "ng test --code-coverage",
    "test-once": "ng test --watch=false --code-coverage",
    "build-dev": "ng build",
    "deploy": "firebase deploy",
    "deploy-dev": "firebase deploy -P dev",
    "lint": "ng lint",
    "duplication": "jscpd --path ./src/ --languages typescript",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:client-and-server-bundles": "ng build --prod && ng run toku-cinema:server:production",
    "build-dev:client-and-server-bundles": "ng build && ng run toku-cinema:server:production",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server && cd functions/ && npm run build && cd ..",
    "build-dev:ssr": "npm run build-dev:client-and-server-bundles && npm run compile:server && cd functions/ && npm run build && cd ..",
    "build-deploy": "npm run build:ssr && npm run deploy",
    "build-deploy-dev": "npm run build-dev:ssr && npm run deploy-dev",
    "use-pre-rendered": "cp dist/pre-render/* -R dist/ && rm -rf dist/pre-render",
    "pre-render": "sleep 1 && node pre-render.js && npm run use-pre-rendered"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.0",
    "@angular/common": "9.1.0",
    "@angular/compiler": "9.1.0",
    "@angular/core": "9.1.0",
    "@angular/fire": "6.0.0",
    "@angular/forms": "9.1.0",
    "@angular/platform-browser": "9.1.0",
    "@angular/platform-browser-dynamic": "9.1.0",
    "@angular/router": "9.1.0",
    "core-js": "^3.6.4",
    "firebase": "^7.13.2",
    "rxjs": "^6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "9.1.0",
    "@angular/language-service": "9.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.11.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "^5.4.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "3.5.3",
    "utf-8-validate": "^5.0.2",
    "webpack-cli": "^3.3.7",
    "ws": "^7.1.2",
    "xhr2": "^0.2.0",
    "puppeteer": "^1.19.0"
  }
}