{
  "name": "toku-cinema",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --prod",
    "test": "ng test --code-coverage",
    "test-once": "ng test --single-run --code-coverage",
    "build-dev": "ng build",
    "deploy": "firebase deploy",
    "deploy-dev": "firebase deploy -P dev",
    "build-deploy": "npm run build && npm run deploy",
    "build-deploy-dev": "npm run build-dev && npm run deploy-dev",
    "lint": "ng lint",
    "duplication": "jscpd --path ./src/ --languages typescript",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run toku-cinema:server:production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.2",
    "@angular/common": "8.2.2",
    "@angular/compiler": "8.2.2",
    "@angular/core": "8.2.2",
    "@angular/fire": "5.1.3",
    "@angular/forms": "8.2.2",
    "@angular/platform-browser": "8.2.2",
    "@angular/platform-browser-dynamic": "8.2.2",
    "@angular/platform-server": "8.2.2",
    "@angular/router": "8.2.2",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "core-js": "^3.2.1",
    "express": "^4.17.1",
    "firebase": "^6.3.5",
    "rxjs": "^6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.1",
    "@angular/cli": "8.2.1",
    "@angular/compiler-cli": "8.2.2",
    "@angular/language-service": "8.2.2",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.7.1",
    "bufferutil": "^4.0.1",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "3.5.3",
    "utf-8-validate": "^5.0.2",
    "webpack-cli": "^3.1.0",
    "ws": "^7.1.2",
    "xhr2": "^0.2.0"
  }
}
