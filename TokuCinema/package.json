{
  "name": "toku-cinema",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --prod",
    "test": "ng test --code-coverage",
    "test-once": "ng test --single-run --code-coverage",
    "build-dev": "ng build",
    "deploy": "firebase deploy",
    "deploy-dev": "firebase deploy -P dev",
    "build-deploy": "npm run build && npm run deploy",
    "build-deploy-dev": "npm run build-dev && npm run deploy-dev",
    "lint": "ng lint",
    "duplication": "jscpd --path ./src/ --languages typescript",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run toku-cinema:server:production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.7",
    "@angular/common": "7.2.7",
    "@angular/compiler": "7.2.7",
    "@angular/core": "7.2.7",
    "@angular/fire": "5.1.1",
    "@angular/forms": "7.2.7",
    "@angular/http": "7.2.7",
    "@angular/platform-browser": "7.2.7",
    "@angular/platform-browser-dynamic": "7.2.7",
    "@angular/platform-server": "7.2.7",
    "@angular/router": "7.2.7",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "0.0.0",
    "core-js": "^2.6.5",
    "express": "^4.15.2",
    "firebase": "5.8.0",
    "rxjs": "^6.4.0",
    "tslib": "^1.9.3",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "7.3.3",
    "@angular/compiler-cli": "7.2.7",
    "@angular/language-service": "7.2.7",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^6.14.3",
    "bufferutil": "^4.0.1",
    "codelyzer": "^4.5.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "ts-loader": "^5.2.0",
    "ts-node": "~3.2.0",
    "tslint": "^5.13.0",
    "typescript": "3.2.2",
    "utf-8-validate": "^5.0.2",
    "webpack-cli": "^3.1.0",
    "ws": "^6.1.2",
    "xhr2": "^0.2.0"
  }
}
